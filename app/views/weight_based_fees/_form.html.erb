
<%= form_with(model: [@shipping_mode, @shipping_mode_weight_based_fee]) do |f| %>
  <div>
  <%= f.label :min_distance %>
  <%= f.number_field :min_distance %>
  </div>
  <div>
  <%= f.label :max_distance %>
  <%= f.number_field :max_distance %>
  </div>
  <div>
  <%= f.label :fee_per_km %>
  <%= f.number_field :fee_per_km, step: 0.01 %>
  </div>
  <%= f.submit 'Enviar' %>
<% end %>
